      "speech_profile_processed": true,
      "stt_provider": null
    },
    {
      "id": "13ed305c-7845-4b2d-96ba-77bea87da8fe",
      "text": "湖の成り立ちと深く関わっています",
      "speaker": "SPEAKER_1",
      "speaker_id": 1,
      "is_user": false,
      "person_id": null,
      "start": 167.05999941825866,
