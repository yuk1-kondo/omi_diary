      "speaker": "SPEAKER_1",
      "speaker_id": 1,
      "is_user": false,
      "person_id": null,
      "start": 190.51999941825866,
      "end": 192.27999941825865,
      "translations": [],
      "speech_profile_processed": true,
      "stt_provider": null
    },
    {
      "id": "b1c3db1c-3b7f-46d5-afe8-6ecf6443f562",
      "text": "ています。",
      "speaker": "SPEAKER_2",
      "speaker_id": 2,
