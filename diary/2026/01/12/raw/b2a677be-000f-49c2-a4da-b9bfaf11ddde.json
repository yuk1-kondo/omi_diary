      "start": 207.49999941825865,
      "end": 215.73999941825866,
      "translations": [],
      "speech_profile_processed": true,
      "stt_provider": null
    },
    {
      "id": "082e758e-6d5c-4278-b103-7018e031ade5",
      "text": "っいうのに見てた街のドラマ。 出る前。",
      "speaker": "SPEAKER_0",
      "speaker_id": 0,
      "is_user": false,
      "person_id": null,
      "start": 219.15999941825868,
