      "text": "湖の底から出てくる穴は",
      "speaker": "SPEAKER_1",
      "speaker_id": 1,
      "is_user": false,
      "person_id": null,
      "start": 186.06000941825866,
      "end": 190.19999941825867,
      "translations": [],
      "speech_profile_processed": true,
